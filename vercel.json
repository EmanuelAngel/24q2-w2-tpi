{
  "version": 2,
  "builds": [
    {
      "src": "src/.bin/www",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "src/**/*.js",
          "src/**/*.mjs"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/.bin/www"
    }
  ]
}
